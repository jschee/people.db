<div class="clearfix">
  <%= render 'shared/sidebar' %>
  <main class="float-right w-5/6 ">
    <p class="font-semibold mb-6">Tags</p>
    <section>
      <% @tags.each do |t| %>
        <%= link_to tags_search_path(tag: t) do %>
          <div class="tag-button rounded duration-300 inline-block"><%= t %></div>
        <% end %>
      <% end %>

    </section>
  </main>
</div>

